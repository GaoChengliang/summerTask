$(function(){
	var tabs = $("#tabs").iframeTabs({'wrapperClass':'tab-wrapper', 'height':660});
	  $("a[name='supplier_manage']").click(function(event){
		  event.preventDefault();
		  var title = $(this).text();
		  tabs.addTab('supplier-manage', '<img src="img/logo/new.png"/>'+ title, 'supplier');
      });
});
