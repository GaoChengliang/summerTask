$(document).ready(function() {
        $("a[name='min']").click(function(){
            Ext.Msg.confirm('提示','你确定要删除该公告吗？',function(button,text){
                if(button=='yes'){
                    window.location.href="/admin/note!delete.action?id=${id}";
                }
            });
        });
        $("a[class='max']").click(function(){
            Ext.Msg.confirm('提示','你确定要删除该公告吗？',function(button,text){
                if(button=='yes'){
                    window.location.href="/admin/note!delete.action?id=${id}";
                }
            });
        });
    });