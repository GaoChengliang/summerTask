$("a[name='view_link']").hover(function(event) {
		$(this).find('img').attr("src", "/img/logo/view_chosen.png");
	}, function(event) {
		$(this).find('img').attr("src", "/img/logo/view.png");
	});

	$("a[name='edit_link']").hover(function(event) {
