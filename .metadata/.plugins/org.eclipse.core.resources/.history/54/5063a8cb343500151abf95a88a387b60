	<%
	if (totalPage <= 0 || currentPage > totalPage) return; 
	var startPage = currentPage - 4;
	if(startPage<1){
		startPage=1;
	}
	var endPage = currentPage+4;
	if(endPage>totalPage){
		endPage = totalPage;
	}
	%>

	<nav>
	<ul class="pagination">
			<%
			if(currentPage<=8){
				startPage = 1;
			}
			if(totalPage-currentPage<8){
				endPage = totalPage;
			}
			if(currentPage==1){			
			%>
			
			<li class="disabled"><a href="#" aria-label="Previous"><span aria-hidden="true">&laquo;</span></a></li>
			<%}else{%>
			 <li><a href="${actionUrl}${currentPage - 1}${urlParas!}" aria-label="Previous"><span aria-hidden="true">&laquo;</span></a></li>
			<%}%>
			
			<%if(currentPage>8){%>
			 <li><a href="${actionUrl}1${urlParas!}">1</a></li>
     		 <li><a href="${actionUrl}2${urlParas!}">2</a></li>
     		 <li><span class="gap">…</span></li>
			<%}%>
			
			<%
			var index = startPage;	
			while(index<=endPage){%>
			    <%if(currentPage == index){%>
			    	<li class="active"><span>${index}</span></li>
				<%}else{%>
					 <li><a href="${actionUrl+index}${urlParas!}">${index}</a></li>
				<%}%>				
				
			<%
			index= index+1;
			}%>
			
			
			<%if((totalPage - currentPage) >= 8){%>
				 <li><span class="gap">…</span></li>
      			 <li><a href="${actionUrl}${totalPage - 1}${urlParas!}">${totalPage - 1}</a></li>
     			 <li><a href="${actionUrl}${totalPage}${urlParas!}">${totalPage}</a></li>
			
		
			<%}%>
			
			<%if(currentPage == totalPage){%>
				  <li class="disabled"><a href="#" aria-label="Next"><span aria-hidden="true">&raquo;</span></a></li>
			<%}else{%>
				  <li><a href="${actionUrl}${currentPage + 1}${urlParas!}" aria-label="Next"><span aria-hidden="true">&raquo;</span></a></li>
			<%}%>
			
	</ul>
	</nav>
